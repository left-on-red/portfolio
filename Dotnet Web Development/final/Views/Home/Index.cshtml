@using Northwind.Models
@model IEnumerable<Discount>

<div class="jumbotron">
    <h1 class="display-4">Welcome to AT corporations</h1>
    <p class="lead">This website allows registered users to manage discounts, by adding editing and deleting them.</p>
    <hr class="my-4">
    <p>To get started, please create an account by clicking the 'register' button in the navigation bar.</p>
</div>
<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/images/discount1Tru.jpg" alt="Photo of various discounts">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/discount2Tru.jpg" alt="Photo of various discounts">
        </div>
        <div class="carousel-item">
            <img class="d-block w-100" src="~/images/discount3Tru.png" alt="Photo of various discounts">
        </div>
    </div>
</div>
</div>
@section Scripts {
<script type="text/javascript" src="~/js/client.js"></script>
}
